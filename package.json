{
  "name": "rollup-typescript-lib-starter",
  "version": "0.5.1",
  "description": "start rollup typescript develop javascript lib",
  "main": "dist/index.js",
  "amd": "dist/index.amd.js",
  "iife": "dist/index.iife.js",
  "umd": "dist/index.umd.js",
  "umdMin": "dist/index.umd.min.js",
  "types": "dist/types/main.d.ts",
  "module": "dist/index.esm.js",
  "exports": {
    "types": "./types/index.d.ts",
    "import": "./dist/index.esm.js",
    "default": "./dist/index.js"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production rollup -c ./config/rollup.config.prod.ts --configPlugin @rollup/plugin-typescript --bundleConfigAsCjs && npm run types && npm run copy",
    "dev": "cross-env NODE_ENV=development rollup -c ./config/rollup.config.dev.ts --configPlugin @rollup/plugin-typescript --bundleConfigAsCjs --watch",
    "docs": "rimraf docs && typedoc --out docs src",
    "analyzer": "cross-env ANALYZER=true npm run build",
    "lint": "eslint src --fix --ext .ts,.tsx,.js,.jsx",
    "fmt": "prettier --write \"{src,__tests__,config}/**/*.{ts,tsx,js,jsx}\"",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "clean": "rimraf docs dist coverage",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "copy": "gulp",
    "types": "tsc src/main.ts --emitDeclarationOnly --declaration --declarationDir dist/types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qqxs/rollup-typescript-lib-starter.git"
  },
  "keywords": [
    "rollup",
    "typescript",
    "lib",
    "swc"
  ],
  "author": "ShineShao <xiaoshaoqq@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qqxs/rollup-typescript-lib-starter/issues"
  },
  "homepage": "https://github.com/qqxs/rollup-typescript-lib-starter#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.4",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-swc": "^0.2.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@skax/camel": "^0.2.5",
    "@swc/core": "^1.3.93",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/user-event": "^14.5.1",
    "@types/eslint": "^8.44.6",
    "@types/jest": "^29.5.6",
    "@types/mime": "^3.0.3",
    "@types/node": "^20.8.7",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "eslint": "^8.51.0",
    "eslint-config-xx": "^1.3.0",
    "gulp": "^4.0.2",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "only-allow": "^1.2.1",
    "postcss": "^8.4.31",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "rimraf": "^5.0.5",
    "rollup": "^4.1.4",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^2.0.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.69.4",
    "through2": "^4.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.2",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist"
  ]
}
