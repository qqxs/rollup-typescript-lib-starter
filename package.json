{
  "name": "rollup-typescript-lib-starter",
  "version": "0.5.8",
  "description": "start rollup typescript develop javascript lib",
  "main": "dist/index.js",
  "amd": "dist/index.amd.js",
  "iife": "dist/index.iife.js",
  "umd": "dist/index.umd.js",
  "umdMin": "dist/index.umd.min.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/index.esm.js",
  "exports": {
    "types": "./types/index.d.ts",
    "import": "./dist/index.esm.js",
    "default": "./dist/index.js"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production rollup -c ./config/rollup.config.prod.ts --configPlugin @rollup/plugin-typescript --bundleConfigAsCjs && npm run copy",
    "dev": "npm run clean && cross-env NODE_ENV=development rollup -c ./config/rollup.config.dev.ts --configPlugin @rollup/plugin-typescript --bundleConfigAsCjs --watch",
    "docs": "rimraf docs && typedoc",
    "analyzer": "cross-env ANALYZER=true npm run build",
    "lint": "eslint src --fix --ext .ts,.tsx,.js,.jsx",
    "fmt": "prettier --write \"{src,__tests__,config}/**/*.{ts,tsx,js,jsx}\"",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "clean": "rimraf docs dist coverage",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "copy": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qqxs/rollup-typescript-lib-starter.git"
  },
  "keywords": [
    "rollup",
    "typescript",
    "lib",
    "swc"
  ],
  "author": "ShineShao <xiaoshaoqq@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qqxs/rollup-typescript-lib-starter/issues"
  },
  "homepage": "https://github.com/qqxs/rollup-typescript-lib-starter#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-eslint": "^9.0.5",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-swc": "^0.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/pluginutils": "^5.1.0",
    "@skax/camel": "^0.2.5",
    "@swc/core": "^1.4.6",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint": "^8.56.5",
    "@types/jest": "^29.5.12",
    "@types/mime": "^3.0.4",
    "@types/node": "^20.11.25",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "cssnano": "^6.1.0",
    "dayjs": "^1.11.10",
    "eslint": "^8.57.0",
    "eslint-config-xx": "^1.9.0",
    "gulp": "^4.0.2",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "only-allow": "^1.2.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^2.0.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.71.1",
    "through2": "^4.0.2",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typedoc": "^0.25.11",
    "typedoc-plugin-rename-defaults": "^0.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  }
}
